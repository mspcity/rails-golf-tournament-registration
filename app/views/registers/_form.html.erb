<%= form_for(@register) do |f| %>
    <%= render partial: 'layouts/errors', locals: {object: @register} %>

<%= f.label :name %>
<%= f.text_field :name %><br>

<%= f.label :email %>
<%= f.text_field :email %><br>

<%= f.label :phone %>
<%= f.text_field :phone %><br>

<%= f.label :age %>
<%= f.text_field :age %><br>

<%= f.label :gender %>
<%= f.text_field :gender %><br>

<br>

<%= f.label :handicap_category %><br>
0 to 5<%= f.radio_button :handicap_category, '20'%><br>
6 to 12<%= f.radio_button :handicap_category, '10 to 20' %><br>
13 to 20<%= f.radio_button :handicap_category, '30 to 40' %><br>
21 to 28<%= f.radio_button :handicap_category, '30 to 40' %><br>
29 to 36<%= f.radio_button :handicap_category, '30 to 40' %><br>
above 36<%= f.radio_button :handicap_category, '30 to 40' %><br>

Select Tournament: <%= f.collection_select :tournament_id, Tournament.all, :id, :name %>


<%= f.submit %>
<% end %>